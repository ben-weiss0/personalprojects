package imageprocessor.model.operations.pixel;

import imageprocessor.model.IPixel;

/**
 * Pixel function object to greyscale by green component.
 */
public class PixelGreyscaleGreen extends PixelTransform {

  @Override
  protected double[][] getTransformationMatrix(IPixel pixel) {
    return new double[][] { {0, 1, 0}, {0, 1, 0}, {0, 1, 0} };
  }
}
